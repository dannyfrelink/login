{
	"name": "login",
	"version": "0.1.0",
	"private": true,
	"proxy": "http://localhost:3001",
	"dependencies": {
		"@emotion/react": "^11.11.3",
		"@emotion/styled": "^11.11.0",
		"@mui/material": "^5.15.2",
		"@testing-library/jest-dom": "^5.17.0",
		"@testing-library/react": "^13.4.0",
		"@testing-library/user-event": "^13.5.0",
		"@types/jest": "^27.5.2",
		"@types/node": "^16.18.69",
		"@types/react": "^18.2.46",
		"@types/react-dom": "^18.2.18",
		"node-sass": "^7.0.3",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-router-dom": "^6.21.1",
		"react-scripts": "5.0.1",
		"sass-loader": "^13.3.3",
		"typescript": "^4.9.5",
		"web-vitals": "^2.1.4"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"predeploy": "npm run build",
		"deploy": "gh-pages -d build",
		"eject": "react-scripts eject",
		"test": "react-scripts test --transformIgnorePatterns \"node_modules/(?!(.*\\.(js|jsx|ts|tsx)$))\"",
		"test:coverage": "react-scripts test --coverage",
		"test:watch": "react-scripts test --watch",
		"test:verbose": "react-scripts test --verbose",
		"test:update": "react-scripts test --updateSnapshot",
		"test:debug": "react-scripts --inspect-brk test --runInBand",
		"start:prod": "serve -s build",
		"prestart:prod": "npm run build",
		"sass:watch": "node-sass --watch src/ -o src/",
		"sass:build": "node-sass src/ -o src/"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"gh-pages": "^6.1.1"
	}
}
